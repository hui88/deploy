      <td width="50" class="align2"><a href="<%= shop_user_path(user.id)%>" target="_blank"><img width="50" height="50" src="<%= user.avatar.url(:small)%>"></a></td>
      <td align="align2" width="150" class="">
      <p class="ware_text"><span idvalue="18831" class="color2" ectype="editobj"><%= user.title%></span></p>                        </td>
      <td width="65"><span class="color2"><%= user.name%></span></td>
      <td width="50" class="align2"><span idvalue="18831" class="color2" ectype="editobj"><%= user.email%></span></td>
      <td width="50" class="align2"><span idvalue="18831" class="color2" ectype="editobj"><%= user.products.size%></span></td>
      <td width="50" class="align2"><span idvalue="18831" class="color2" ectype="editobj"><%= user.orders.size%></span></td>
      <td width="30" class="align2"><span idvalue="18831" style="margin: 0px 5px;" ectype="editobj"  status="on"></span></td>
      <td width="30" class="align2"><span idvalue="18831" style="margin: 0px 5px;" ectype="editobj"  status="on"></span></td>
      <td width="13" class="align2"><span style="margin: 0px 5px;" ></span></td>
      <td class="align2"></td>
      <td>
        <div>
          <li><%= link_to 'Products', user_products_path(user.id) %></li>
          <li><%= link_to 'Orders', user_orders_path(user.id) %></li>
          <li><%= link_to 'Edit', edit_user_path(user) %> </li>
          <li><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></li>
        </div>
      </td>
    </tr>
      
   <%end%>

