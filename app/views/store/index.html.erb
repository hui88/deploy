<h1>Your Pragmatic Catalog</h1>
<table>
  <tr>
    <th>Image</th>
    <th>Title</th>
    <th>Description</th>
    <th>Image url</th>
    <th>Price</th>
    <th>Action</th>
 </tr>

<% @products.each do |product| %>
  <tr>
    <td><%= image_tag product.image.url(:small) %></td>
    <td><%=h product.title %></td>
    <td><%=h product.description %></td>
    <td><img class="list-image" src="<%=h product.image_url %>"/></td>
    <td><%=h product.price %></td>    
    <td><%= link_to 'show',:action =>"show",:id =>product.id  %></td> 
 </tr>
 
<% end %>
